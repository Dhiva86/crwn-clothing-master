{
  "name": "crwn-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "v18.16.0",
    "npm": "9.5.1"
  },
  "scripts": {
    "type": "module",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-sslify": "^1.2.0",
    "stripe": "13.5.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  }
}
